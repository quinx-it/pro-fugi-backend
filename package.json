    "amqp-connection-manager": "5.0.0",
    "amqplib": "0.10.9",
    "axios": "1.8.2",
    "bcryptjs": "3.0.2",
    "bullmq": "5.56.1",
    "cache-manager": "7.0.0",
    "camelcase-keys": "10.0.1",
