  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "nest start",
    "build": "nest build",
    "prepare": "husky && echo \"npx lint-staged && pnpm typechecking\" > .husky/_/pre-commit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "migration:generate": "typeorm migration:generate -d dist/configs/db.config.js src/migrations/migration",
    "migration:generate:build": "run-s build migration:generate",
    "migration:create": "typeorm migration:create src/migrations/migration",
    "migration:run": "typeorm migration:run -d dist/configs/db.config.js",
    "migration:run:build": "run-s build migration:run",
    "migration:revert": "typeorm migration:revert -d dist/configs/db.config.js",
    "migration:revert:build": "run-s build migration:revert",
    "typechecking": "tsc --noEmit"
  },
  "engines": {
    "node": "22.x"
  },
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
